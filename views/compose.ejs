<%- include('partials/header') -%> 
<h1>Compose</h1>
<form action="/compose" method="post">
    <label for="postTitle">Title</label>
    <input class="form-control" name="postTitle" type="text" autocomplete="off">
    <label for="postBody">Post</label>
    <textarea class="form-control" name="postBody" cols="30" rows="8"></textarea>
    <button class="btn btn-primary" id="btn" type="submit">Publish</button>
</form>

<%- include('partials/footer') -%> 